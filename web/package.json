{
  "private": true,
  "name": "sanity-sapper-blog-web",
  "description": "Sapper frontend",
  "version": "0.0.21",
  "scripts": {
    "dev": "run-p watch:*",
    "watch:css": "postcss src/assets/global.pcss -o static/global.css -w",
    "watch:dev": "sapper dev",
    "build": "run-s build:css build:sapper",
    "build:css": "postcss src/assets/global.pcss -o static/global.css",
    "build:sapper": "sapper build --legacy",
    "build:export": "sapper export --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "serve": "serve ___sapper__/export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@sanity/client": "^2.1.4",
    "@sanity/image-url": "^0.140.19",
    "@tailwindcss/typography": "^0.4.0",
    "compression": "^1.7.4",
    "groq": "^2.1.4",
    "polka": "^1.0.0-next.11",
    "sirv": "^1.0.10",
    "svelte-material-icons": "^1.0.3",
    "tailwindcss": "^2.0.2",
    "uninstall": "0.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "@movingbrands/svelte-portable-text": "0.0.11",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@sanity/block-content-to-hyperscript": "^2.0.10",
    "autoprefixer": "^10.2.3",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rollup": "^2.38.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.0",
    "svelte": "^3.31.2",
    "svelte-click-outside": "^1.0.0",
    "svelte-loader": "^3.0.0",
    "svelte-preprocess": "^4.6.3"
  }
}
